    <script>
        document.querySelectorAll('.radio-group').forEach(radioGroup => {
            const radios = radioGroup.querySelectorAll('input[type="radio"]');

            radios.forEach(radio => {
                radio.addEventListener('change', () => {
                    // Remove 'selected' from all labels in this group
                    radios.forEach(r => r.parentElement.classList.remove('selected'));
                    // Add 'selected' to the checked radio's label
                    if (radio.checked) {
                        radio.parentElement.classList.add('selected');
                    }
                });

                // Trigger initial state on page load if any radio is checked
                if (radio.checked) {
                    radio.parentElement.classList.add('selected');
                }
            });
        });

    </script>
    <script>
        document.addEventListener("DOMContentLoaded", () => {
            // Show/hide stream fields based on last class selection
            const lastClassRadios = document.getElementsByName("lastClass");
            const streamFields = document.getElementById("streamFields");

            lastClassRadios.forEach(radio => {
                radio.addEventListener("change", () => {
                    if (radio.value === "X" || radio.value === "XI") {
                        streamFields.style.display = "block";
