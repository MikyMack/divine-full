                    <script>
                        function sendToWhatsApp(event) {
                            event.preventDefault();

                            const childName = document.getElementById("childName").value;
                            const dob = document.getElementById("dob").value;
                            const parentName = document.getElementById("parentName").value;
                            const designation = document.getElementById("designation").value;
                            const email = document.getElementById("email").value;
                            const phone = document.getElementById("phone").value;
                            const division = document.getElementById("division").value;
                            const notice = document.getElementById("notice").checked ? "Yes" : "No";

                            const message = `*New Admission Request:*
                      👶 *Child's Name:* ${childName}
                      🎂 *DOB:* ${dob}
                      👨‍👩‍👧 *Parent's Name:* ${parentName}
                      💼 *Parent's Designation:* ${designation}
                      📧 *Email:* ${email}
                      📱 *Phone:* ${phone}
                      🏫 *Division to Join:* ${division}
                      📋 *Weekly Progress Notification:* ${notice}`;

                            const phoneNumber = "919895361126"; // WhatsApp number in international format without '+'
                            const url = `https://wa.me/${phoneNumber}?text=${encodeURIComponent(message)}`;

