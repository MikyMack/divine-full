        function sendAdmissionToWhatsApp(event) {
            event.preventDefault();

            // Parent Information
            const name = document.getElementById('name').value.trim();
            const email = document.getElementById('email').value.trim();
            const contact = document.getElementById('contact').value.trim();

            // Student Information
            const childName = document.getElementById('childName').value.trim();
            const age = document.getElementById('age').value.trim();
            const classToJoin = document.getElementById('class').value.trim();
            const lastSchool = document.getElementById('lastSchool').value.trim();

            // Academic Background
            const board = document.querySelector('input[name="board"]:checked')?.value || 'Not specified';
            const lastClass = document.querySelector('input[name="lastClass"]:checked')?.value || 'Not specified';

            // Stream Information (if applicable)
            let stream = '';
            let classApplied = '';
            if (document.getElementById('streamFields').style.display === 'block') {
                stream = document.querySelector('input[name="stream"]:checked')?.value || 'Not specified';
                classApplied = document.querySelector('input[name="classApplied"]:checked')?.value || 'Not specified';
            }

            // Construct WhatsApp message
            let message = `📚 *New Admission Request* 📚%0A%0A` +
                 `*👤 Parent Information*%0A` +
                 `Name: ${encodeURIComponent(name)}%0A` +
                 `Email: ${encodeURIComponent(email)}%0A` +
                 `Contact: ${encodeURIComponent(contact)}%0A%0A` +
                 `*👶 Student Information*%0A` +
                 `Name: ${encodeURIComponent(childName)}%0A` +
                 `Age: ${encodeURIComponent(age)}%0A` +
                 `Class to Join: ${encodeURIComponent(classToJoin)}%0A` +
                 `Last School: ${encodeURIComponent(lastSchool || 'N/A')}%0A%0A` +
                 `*📝 Academic Background*%0A` +
                 `Board: ${encodeURIComponent(board)}%0A` +
                 `Last Class: ${encodeURIComponent(lastClass)}`;

            // Add stream info if applicable
            if (stream && classApplied) {
                message += `%0AStream: ${encodeURIComponent(stream)}` +
                    `%0AClass Applied: ${encodeURIComponent(classApplied)}`;
            }

            // Replace with your WhatsApp number (with country code)
            const whatsappNumber = "919895361126";
            const whatsappUrl = `https://wa.me/${whatsappNumber}?text=${message}`;

            // Open WhatsApp in new tab
            window.open(whatsappUrl, "_blank");

            // Close popup and reset form
            document.getElementById('admissionPopup').style.display = 'none';
            document.body.style.overflow = '';
            event.target.reset();

