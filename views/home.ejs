    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Gallery modal elements
            const galleryModal = document.createElement('div');
            galleryModal.className = 'insta-modal';
            galleryModal.style.display = 'none';
            galleryModal.innerHTML = `
        <div class="insta-modal-content">
            <span class="insta-close">&times;</span>
            <img class="insta-modal-image" src="" alt="">
            <div class="insta-modal-nav">
                <button class="insta-nav-btn insta-prev-btn">&#10094;</button>
                <button class="insta-nav-btn insta-next-btn">&#10095;</button>
            </div>
            <div class="insta-modal-caption">
                <span class="insta-current-index">1</span>/<span class="insta-total-count">0</span>
                <p class="insta-caption-text"></p>
            </div>
        </div>
    `;
            document.body.appendChild(galleryModal);

            // Get all gallery items
            const galleryItems = document.querySelectorAll('.insta-card');
            const totalItems = galleryItems.length;
            document.querySelector('.insta-total-count').textContent = totalItems;

            let currentIndex = 0;

            // Open modal with clicked image
            function openModal(index) {
                currentIndex = index;
                const item = galleryItems[index];
                const imgSrc = item.querySelector('img').src;
                const caption = item.querySelector('.insta-caption p').textContent;

                galleryModal.querySelector('.insta-modal-image').src = imgSrc;
                galleryModal.querySelector('.insta-caption-text').textContent = caption;
                galleryModal.querySelector('.insta-current-index').textContent = index + 1;
                galleryModal.style.display = 'flex';
                document.body.style.overflow = 'hidden';

                // Fade in effect
                setTimeout(() => {
                    galleryModal.style.opacity = '1';
                }, 10);
            }

            // Close modal
            function closeModal() {
                galleryModal.style.opacity = '0';
                setTimeout(() => {
                    galleryModal.style.display = 'none';
                    document.body.style.overflow = 'auto';
                }, 300);
            }

            // Navigate between images
            function navigate(direction) {
                currentIndex += direction;

                // Circular navigation
                if (currentIndex >= totalItems) currentIndex = 0;
                if (currentIndex < 0) currentIndex = totalItems - 1;

                openModal(currentIndex);
            }

            // Event listeners for gallery items
            galleryItems.forEach((item, index) => {
                item.addEventListener('click', (e) => {
                    // Don't open if clicking on like/comment buttons
                    if (!e.target.closest('.insta-likes, .insta-comments')) {
                        openModal(index);
                    }
                });
            });

            // Modal controls
            galleryModal.querySelector('.insta-close').addEventListener('click', closeModal);
            galleryModal.querySelector('.insta-prev-btn').addEventListener('click', () => navigate(-1));
            galleryModal.querySelector('.insta-next-btn').addEventListener('click', () => navigate(1));

            // Keyboard navigation
            document.addEventListener('keydown', (e) => {
                if (galleryModal.style.display === 'flex') {
                    if (e.key === 'Escape') closeModal();
                    if (e.key === 'ArrowLeft') navigate(-1);
                    if (e.key === 'ArrowRight') navigate(1);
                }
            });

            // Close when clicking outside image
            galleryModal.addEventListener('click', (e) => {
                if (e.target === galleryModal) {
                    closeModal();
                }
            });
        });
    </script>
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Get elements
            const slides = document.querySelectorAll('.prlx_slide');
            const dots = document.querySelectorAll('.prlx_nav_dot');
            const prevArrow = document.querySelector('.prlx_arrow_prev');
            const nextArrow = document.querySelector('.prlx_arrow_next');

            let currentSlide = 0;
            let slideInterval = null;

            // Initialize slider
            function initSlider() {
                // Set active slide
                updateSlide();

                // Start auto sliding
                startAutoSlide();

                // Add event listeners
                dots.forEach(dot => {
                    dot.addEventListener('click', () => {
                        currentSlide = parseInt(dot.dataset.index);
                        updateSlide();
                        resetAutoSlide();
                    });
                });

                prevArrow.addEventListener('click', () => {
                    currentSlide = (currentSlide - 1 + slides.length) % slides.length;
                    updateSlide();
                    resetAutoSlide();
                });

                nextArrow.addEventListener('click', () => {
                    currentSlide = (currentSlide + 1) % slides.length;
                    updateSlide();
                    resetAutoSlide();
                });


            }

            // Update slide state
            function updateSlide() {
                slides.forEach((slide, index) => {
                    if (index === currentSlide) {
                        slide.classList.add('prlx_active');
                    } else {
                        slide.classList.remove('prlx_active');
                    }
                });

                dots.forEach((dot, index) => {
                    if (index === currentSlide) {
                        dot.classList.add('prlx_active');
                    } else {
                        dot.classList.remove('prlx_active');
                    }
                });
            }

            // Auto slide functionality
            function startAutoSlide() {
                slideInterval = setInterval(() => {
                    currentSlide = (currentSlide + 1) % slides.length;
                    updateSlide();
                }, 5000);
            }

            function resetAutoSlide() {
                clearInterval(slideInterval);
                startAutoSlide();
            }

            // Parallax effect


            // Initialize the slider
            initSlider();
        });
    </script>

    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const track = document.querySelector('.dyno-announcement-track');
            const cards = document.querySelectorAll('.dyno-announce-card');
            const dots = document.querySelectorAll('.dyno-announce-dot');
            const prevBtn = document.querySelector('.dyno-announce-prev');
            const nextBtn = document.querySelector('.dyno-announce-next');

            let currentIndex = 0;
            const cardCount = cards.length;

            // Update carousel position
            function updateCarousel() {
                track.style.transform = `translateX(-${currentIndex * 100}%)`;

                // Update dots
                dots.forEach((dot, index) => {
                    dot.classList.toggle('active', index === currentIndex);
                });
            }

            // Next button click
            nextBtn.addEventListener('click', () => {
                currentIndex = (currentIndex + 1) % cardCount;
                updateCarousel();
            });

            // Previous button click
            prevBtn.addEventListener('click', () => {
                currentIndex = (currentIndex - 1 + cardCount) % cardCount;
                updateCarousel();
            });

            // Dot navigation
            dots.forEach((dot, index) => {
                dot.addEventListener('click', () => {
                    currentIndex = index;
                    updateCarousel();
                });
            });

            // Auto-advance every 5 seconds
            setInterval(() => {
                currentIndex = (currentIndex + 1) % cardCount;
                updateCarousel();
            }, 5000);
        });
        const dots = document.querySelectorAll('.dot');
        let currentDot = 0;

        setInterval(() => {
            dots.forEach((dot, index) => {
                dot.classList.remove('active');
            });

            currentDot = (currentDot + 1) % dots.length;
            dots[currentDot].classList.add('active');
        }, 1500);
    </script>
